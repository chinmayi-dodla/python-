def vote(n):
   if n>18:
     print("Yes")
   else:
     print("No")
   n=int(input("Enter the age"))
   vote(n)
             (Or)

def vote(n):
   if n>18:
     return True 
   else:
     return False
   n=int(input("Enter the age"))
   print(vote(n))
              (Or)
def vote(n):
   if n>18:
     return True 
   else:
     return False
   n=int(input("Enter the age"))
   if(vote(n)):
      print("Yes")
   else:
     print("No")
